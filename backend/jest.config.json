{
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
        "**/*.js",
        "!node_modules/**",
        "!coverage/**",
        "!jest.config.js"
    ],
    "testMatch": [
        "**/__tests__/**/*.test.js"
    ],
    "transform": {},
    "moduleNameMapper": {
        "^(\\.{1,2}/.*)\\.js$": "$1"
    }
}